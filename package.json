{
  "name": "social-proof-section-master",
  "version": "1.0.0",
  "description": "![Design preview for the Social proof section coding challenge](./design/desktop-preview.jpg)",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass ./sass/main.scss ./css/style.css -w sass",
    "compress:sass": "node-sass ./sass/main.scss ./css/style.css --output-style compressed",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" ./css/style.css -o ./css/style-prefixed.css",
    "compress:css": "node-sass ./css/style-prefixed.css ./css/style.css --output-style compressed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.0.2",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.2.0"
  }
}
